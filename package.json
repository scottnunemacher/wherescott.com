{
  "name": "wherescott.com",
  "version": "1.0.0",
  "description": "wherescott.com - Where Scott Does Everything",
  "main": ".eleventy.js",
  "scripts": {
    "clean": "rm -rf public",
    "watch:sass": "sass --no-source-map --watch src/assets/sass:assets/css",
    "build:sass": "sass --no-source-map src/assets/sass:assets/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all clean --parallel watch:*",
    "build": "npm-run-all clean build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottnunemacher/wherescott.com.git"
  },
  "author": "Scott Nunemacher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scottnunemacher/wherescott.com/issues"
  },
  "homepage": "https://github.com/scottnunemacher/wherescott.com#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@picocss/pico": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "sass": "^1.79.3"
  }
}
